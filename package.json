{
  "name": "n8n-nodes-flowbot",
  "version": "3.0.1",
  "description": "Flowbot custom nodes for n8n",
  "keywords": [
    "flowbot",
    "n8n-community-node-package",
    "n8n-nodes-flowbot",
    "n8n-nodes",
    "n8n",
    "n8n.io",
    "n8n.io-nodes"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:n8n": "eslint --ext .ts .",
    "test": "jest",
    "prepare": "npm run build"
  },
  "author": "Flowbot Developer <info@appstogo.net> (https://appstogo.net/flowbot-connector)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/appstogonet/flowbot-n8n-app"
  },
  "bugs": {
    "url": "https://github.com/appstogonet/flowbot-n8n-app/issues"
  },
  "homepage": "https://github.com/appstogonet/flowbot-n8n-app#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18 <23"
  },
  "files": [
    "dist/",
    "nodes/",
    "shared/",
    "credentials/",
    "LICENSE",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/FlowbotApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CreateTicketTrigger/CreateTicketTrigger.node.js",
      "dist/nodes/UpdateTicketTrigger/UpdateTicketTrigger.node.js",
      "dist/nodes/StartTypingSoundAction/StartTypingSoundAction.node.js",
      "dist/nodes/StopTypingSoundAction/StopTypingSoundAction.node.js",
      "dist/index.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "n8n-workflow": "^1.17.0"
  }
}
